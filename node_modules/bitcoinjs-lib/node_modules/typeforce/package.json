    "name": "dcousens",
    "email": "email@dcousens.com"
  },
  "dist": {
    "shasum": "84f6ba427598095d84d907ceca86fda4f75cd522",
    "tarball": "http://registry.npmjs.org/typeforce/-/typeforce-1.3.6.tgz"
  },
  "maintainers": [
    {
      "name": "dcousens",
      "email": "email@dcousens.com"
    }
  ],
